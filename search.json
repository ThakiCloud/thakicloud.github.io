---
layout: null
sitemap: false
---

[
  {% assign posts = site.posts | where: "lang", "ko" %}
  {% for post in posts %}
    {
      "title": {{ post.title | jsonify }},
      "url": {{ post.url | relative_url | jsonify }},
      "date": {{ post.date | date: "%Y-%m-%d" | jsonify }},
      "categories": {{ post.categories | jsonify }},
      "tags": {{ post.tags | jsonify }},
      "content": {{ post.content | prepare_search_content | jsonify }},
      "excerpt": {{ post.excerpt | strip_html | truncatewords: 50 | prepare_search_content | jsonify }},
      "brand_keywords": "Thaki Cloud, ThakiCloud, 다키클라우드, thaki cloud, thaki-cloud, THAKI CLOUD"
    }{% unless forloop.last %},{% endunless %}
  {% endfor %}
]